{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@Store/*": ["src/store/*"],
            "@Navigations/*": ["src/navigations/*"],
            "@Screens/*": ["src/screens/*"],
            "@Hooks/*": ["src/hooks/*"],
            "@Api/*": ["src/api/*"],
            "@Assets/*": ["assets/*"]
        }
    }
}
